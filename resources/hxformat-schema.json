				},
				"leftCurly": {
					"description": "global left curly line end setting",
					"$ref": "#/definitions/formatter.config.LeftCurlyLineEndPolicy",
					"default": "after"
				},
				"blockCurly": {
					"description": "line end settings for block curlies",
