				},
				"sharp": {
					"$ref": "#/definitions/formatter.config.SharpLineEndPolicy",
					"default": "after"
				},
				"emptyCurly": {
					"description": "global empty curlies line end setting",
					"$ref": "#/definitions/formatter.config.EmptyCurlyPolicy",
