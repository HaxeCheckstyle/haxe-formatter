class Main {
	static function main() {
		window.showErrorMessage("No Haxe configurations are available. Please provide the haxe.displayConfigurations setting.",
			({title: "Edit settings"} : vscode.MessageItem)).then(function(button) {
				if (button == null)
					return;
			})
		);
	}
}

