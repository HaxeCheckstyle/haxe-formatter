---

class Main {
	function testDollarVarInCase() {
		check("
class Foo {
    macro function foo(e) {
        switch (e) {
            case macro %$expr%:
                %expr%;
        }
    }
}", "
class Foo {
    macro function foo(e) {
        switch (e) {
            case macro $newName:
                newName;
        }
    }
}");
	}
}

